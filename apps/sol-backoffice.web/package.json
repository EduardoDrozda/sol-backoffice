{
  "name": "@sol-backoffice/sol-backoffice.web",
  "version": "0.0.0",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "@angular-devkit/build-angular:browser",
        "outputs": ["{options.outputPath}"],
        "options": {
          "outputPath": "dist/apps/sol-backoffice.web",
          "index": "apps/sol-backoffice.web/src/index.html",
          "main": "apps/sol-backoffice.web/src/main.ts",
          "polyfills": ["zone.js"],
          "tsConfig": "apps/sol-backoffice.web/tsconfig.app.json",
          "inlineStyleLanguage": "scss",
          "assets": [
            {
              "glob": "**/*",
              "input": "apps/sol-backoffice.web/public"
            }
          ],
          "styles": [
            "apps/sol-backoffice.web/src/styles.scss",
            "apps/sol-backoffice.web/src/styles/main.scss"
          ],
          "stylePreprocessorOptions": {
            "includePaths": ["apps/sol-backoffice.web/src/styles"]
          },
          "scripts": []
        },
        "configurations": {
          "production": {
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "500kB",
                "maximumError": "1MB"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "20kB",
                "maximumError": "30kB"
              }
            ],
            "outputHashing": "all",
            "serviceWorker": true
          },
          "development": {
            "optimization": false,
            "extractLicenses": false,
            "sourceMap": true,
            "fileReplacements": [
              {
                "replace": "apps/sol-backoffice.web/src/environments/environment.ts",
                "with": "apps/sol-backoffice.web/src/environments/environment.development.ts"
              }
            ]
          }
        },
        "defaultConfiguration": "production"
      },
      "serve": {
        "executor": "@angular-devkit/build-angular:dev-server",
        "configurations": {
          "production": {
            "buildTarget": "@sol-backoffice/sol-backoffice.web:build:production"
          },
          "development": {
            "buildTarget": "@sol-backoffice/sol-backoffice.web:build:development"
          }
        },
        "defaultConfiguration": "development"
      },
      "extract-i18n": {
        "executor": "@angular-devkit/build-angular:extract-i18n"
      },
      "test": {
        "executor": "@angular-devkit/build-angular:karma",
        "options": {
          "polyfills": ["zone.js", "zone.js/testing"],
          "tsConfig": "apps/sol-backoffice.web/tsconfig.spec.json",
          "inlineStyleLanguage": "scss",
          "assets": [
            {
              "glob": "**/*",
              "input": "apps/sol-backoffice.web/public"
            }
          ],
          "styles": [
            "apps/sol-backoffice.web/src/styles.scss",
            "apps/sol-backoffice.web/src/styles/main.scss"
          ],
          "stylePreprocessorOptions": {
            "includePaths": ["apps/sol-backoffice.web/src/styles"]
          },
          "scripts": []
        }
      }
    }
  },
  "dependencies": {}
}
